proc _tpl(_tpl_ text, variadic _bindings_ varchar[]) RETURNS text
  result text := _tpl_;
  BEGIN
    FOR i IN 1..(array_upper(_bindings_, 1)/2) LOOP
      result := replace(result, '{{' || _bindings_[i*2 - 1] || '}}', coalesce(_bindings_[i*2], ''));
    END LOOP;
    RETURN result;

